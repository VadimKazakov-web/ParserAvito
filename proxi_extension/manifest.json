{
    "version": "2.0.0",
    "manifest_version": 3,
    "name": "Chrome Proxy",
    "permissions": [
        "proxy",
        "tabs",
        "unlimitedStorage",
        "storage",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "webRequestBlocking",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "action": {
        "default_popup": "hello.html",
        "default_icon": "./hello_extensions.png"
      },
    "background": {
        "scripts": ["test.js"]
    },
    "minimum_chrome_version": "22.0.0"
}